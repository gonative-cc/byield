{
	"name": "byiled",
	"license": "MPL-2.0",
	"private": true,
	"type": "module",
	"packageManager": "bun@1.3.1",
	"scripts": {
		"prepare": "git submodule update --init; cd .git/hooks; ln -s -f ../../contrib/git-hooks/pre-commit ./",
		"submodule:upgrade": "git submodule update --remote",
		"cf-typegen": "wrangler types",
		"build": "react-router build",
		"build:dev": "react-router build --mode dev",
		"deploy": "rm -rf build && bun run build --mode dev && wrangler deploy --env=\"\"",
		"deploy:prod:legacy": "rm -rf build && bun run build && wrangler deploy -c ./wrangler-prod.jsonc",
		"deploy:prod": "CLOUDFLARE_ENV=prod bun run build && wrangler deploy",
		"deploy:prod:ver": "bun run build && wrangler versions upload -c ./wrangler-prod.jsonc",
		"preview": "vite preview",
		"preview:wrangler": "wrangler dev",
		"dev": "CLOUDFLARE_ENV=dev react-router dev",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"lint:fix": "bun run lint --fix",
		"format": "prettier --write .",
		"start": "wrangler dev",
		"typecheck": "react-router typegen && tsc -b",
		"db:migrate": "wrangler d1 migrations apply BeelieversD1",
		"test": "vitest"
	},
	"dependencies": {
		"@ethersproject/units": "^5.8.0",
		"@mysten/dapp-kit": "^0.19.9",
		"@mysten/kiosk": "^0.14.4",
		"@mysten/sui": "^1.45.0",
		"@react-router/cloudflare": "^7.9.6",
		"@react-router/remix-routes-option-adapter": "^7.9.6",
		"@tanstack/react-query": "^5.90.9",
		"@types/react-table": "^7.7.20",
		"bip32": "5.0.0",
		"bitcoinjs-lib": "^7.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"ecpair": "^3.0.0",
		"isbot": "^5.1.32",
		"lucide-react": "^0.552.0",
		"process": "^0.11.10",
		"react": "^19.2.0",
		"react-daisyui": "^5.0.5",
		"react-dom": "^19.2.0",
		"react-hook-form": "^7.66.0",
		"react-number-format": "^5.4.4",
		"react-router": "^7.9.6",
		"react-table": "^7.8.0",
		"sats-connect": "^4.2.1",
		"tailwind-merge": "^3.4.0",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.14.2",
		"@cloudflare/vitest-pool-workers": "^0.10.7",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@mysten/utils": "^0.2.0",
		"@react-router/dev": "^7.9.6",
		"@tailwindcss/postcss": "^4.1.17",
		"@tailwindcss/vite": "^4.1.17",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.4",
		"@types/react-dom": "^19.2.3",
		"@types/react-router": "^5.1.20",
		"daisyui": "^5.5.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-tailwindcss": "^4.0.0-beta.0",
		"fast-check": "^4.3.0",
		"globals": "^16.5.0",
		"jiti": "^2.6.1",
		"jsdom": "^27.2.0",
		"lint-staged": "^16.2.6",
		"miniflare": "^4.20251109.1",
		"postcss": "^8.5.6",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.4",
		"vite": "^6.4.1",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.9",
		"wrangler": "^4.48.0"
	},
	"lint-staged": {
		"*.{js,cjs,mjs,jsx,ts,tsx}": [
			"prettier --write --ignore-unknown",
			"eslint --cache --cache-location ./node_modules/.cache/eslint --fix"
		],
		"*.{json,html}": [
			"prettier --write --ignore-unknown"
		]
	}
}
