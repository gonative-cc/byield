{
	"name": "byiled",
	"license": "MPL-2.0",
	"private": true,
	"type": "module",
	"packageManager": "bun@1.2.20",
	"scripts": {
		"prepare": "cd .git/hooks; ln -s -f ../../contrib/git-hooks/pre-commit ./",
		"postinstall": "bun run cf-typegen",
		"cf-typegen": "wrangler types",
		"build": "react-router build",
		"build:dev": "react-router build --mode dev",
		"deploy": "bun run build --mode dev && wrangler deploy",
		"deploy:prod:legacy": "bun run build && wrangler deploy -c ./wrangler-prod.jsonc",
		"deploy:prod": "bun run build && wrangler deploy -e prod",
		"deploy:prod:ver": "bun run build && wrangler versions upload -c ./wrangler-prod.jsonc",
		"preview": "vite preview",
		"preview:wrangler": "wrangler dev",
		"dev": "react-router dev",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"format": "prettier --write .",
		"start": "wrangler dev",
		"typecheck": "react-router typegen && tsc -b",
		"db:migrate": "wrangler d1 migrations apply BeelieversD1",
		"test": "vitest"
	},
	"dependencies": {
		"@ethersproject/units": "^5.8.0",
		"@mysten/dapp-kit": "^0.18.0",
		"@mysten/kiosk": "^0.13.1",
		"@mysten/sui": "^1.38.0",
		"@react-router/cloudflare": "^7.9.1",
		"@react-router/remix-routes-option-adapter": "^7.9.1",
		"@tanstack/react-query": "^5.90.1",
		"@types/react-table": "^7.7.20",
		"axios": "^1.12.2",
		"bip32": "5.0.0-rc.0",
		"bitcoinjs-lib": "^6.1.7",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"ecpair": "^3.0.0",
		"isbot": "^5.1.30",
		"lucide-react": "^0.544.0",
		"react": "^19.1.1",
		"react-daisyui": "^5.0.5",
		"react-dom": "^19.1.1",
		"react-hook-form": "^7.63.0",
		"react-number-format": "^5.4.4",
		"react-router": "^7.9.1",
		"react-table": "^7.8.0",
		"sats-connect": "^3.6.1",
		"tailwind-merge": "^3.3.1",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.13.3",
		"@cloudflare/vitest-pool-workers": "^0.9.3",
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.36.0",
		"@mysten/utils": "^0.2.0",
		"@react-router/dev": "^7.9.1",
		"@tailwindcss/postcss": "^4.1.13",
		"@tailwindcss/vite": "^4.1.13",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.5.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"@types/react-router": "^5.1.20",
		"daisyui": "^5.1.14",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"fast-check": "^4.3.0",
		"globals": "^16.4.0",
		"jiti": "^2.6.0",
		"jsdom": "^27.0.0",
		"lint-staged": "^16.2.0",
		"miniflare": "^4.20250917.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^6.3.6",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4",
		"wrangler": "^4.38.0"
	},
	"lint-staged": {
		"*.{js,cjs,mjs,jsx,ts,tsx}": [
			"prettier --write --ignore-unknown",
			"eslint --cache --cache-location ./node_modules/.cache/eslint --fix"
		],
		"*.{json,html}": [
			"prettier --write --ignore-unknown"
		]
	}
}
